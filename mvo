// Braces and brackets on F11 and F22, Swap ÃŸ and \, Alt and Meta
partial modifier_keys
xkb_symbols "tweaks" {
    key <FK11> {
        type= "CTRL+ALT",
        symbols[Group1]= [             bracketleft,             braceleft,             F11,             F11, XF86Switch_VT_11 ]
    };
    key <FK12> {
        type= "CTRL+ALT",
        symbols[Group1]= [             bracketright,             braceright,             F12,             F12, XF86Switch_VT_12 ]
    };
    key <AE11> {
        type= "FOUR_LEVEL_PLUS_LOCK",
        symbols[Group1]= [          backslash,        question,       ssharp,    questiondown,           U1E9E ]
    };
    key <LALT> {
        type= "ONE_LEVEL",
        symbols[Group1]= [         Hyper_L ]
    };
    key <RCTL> {         [           Alt_R ] };
    modifier_map Mod1 { <RCTL> };
    modifier_map Mod3 { <HYPR> };
};
